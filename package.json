{
  "name": "node_ondemand-app",
  "version": "1.0.0",
  "main": "./src/index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard --fix",
    "test": "yarn lint && ava **/__test__/**/*.test.js",
    "test:watch": "yarn lint && ava **/__test__/**/*.test.js --watch",
    "precommit": "yarn test && git add .",
    "build": "rm -rf dist && babel src -d dist --ignore __test__",
    "start": "nodemon -r babel-register src/index.js",
    "start:production": "node dist/index.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.14.11",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "joi": "^13.1.0",
    "mongoose": "^5.0.0"
  }
}
